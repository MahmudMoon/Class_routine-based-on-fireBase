<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="15dp"
    android:layout_marginRight="15dp">

    <TextView
        android:text="@string/th"
        style="@style/titleView"
        android:id="@+id/thu_title"
       />


    <TextView
        android:layout_below="@+id/tue_ttitle"
        android:id="@+id/at_class_time_thu"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:ems="10"
        android:layout_marginTop="100dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        style="@style/Time_textView"
        />

    <TextView
        android:id="@+id/ast_class_thu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:ems="10"
        android:layout_alignBaseline="@+id/at_class_time_thu"
        android:layout_alignBottom="@+id/at_class_time_thu"
        android:layout_toEndOf="@+id/at_class_time_thu"
        style="@style/class_textView"
        />

    <TextView
        android:id="@+id/bt_class_time_thu"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:ems="10"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/at_class_time_thu"
        style="@style/Time_textView"/>

    <TextView
        android:id="@+id/bst_class_thu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:ems="10"
        android:layout_alignBaseline="@+id/bt_class_time_thu"
        android:layout_alignBottom="@+id/bt_class_time_thu"
        android:layout_toEndOf="@+id/bt_class_time_thu"
        style="@style/class_textView"/>

    <TextView
        android:id="@+id/ct_class_time_thu"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:ems="10"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/bt_class_time_thu"
        style="@style/Time_textView"/>

    <TextView
        android:id="@+id/cst_class_thu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:ems="10"
        android:layout_alignBaseline="@+id/ct_class_time_thu"
        android:layout_alignBottom="@+id/ct_class_time_thu"
        android:layout_toEndOf="@+id/ct_class_time_thu"
        style="@style/class_textView"
        />

    <TextView
        android:id="@+id/dt_class_time_thu"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:ems="10"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/ct_class_time_thu"
        style="@style/Time_textView"/>

    <TextView
        android:id="@+id/dst_class_thu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:ems="10"
        android:layout_alignBaseline="@+id/dt_class_time_thu"
        android:layout_alignBottom="@+id/dt_class_time_thu"
        android:layout_toEndOf="@+id/dt_class_time_thu"
        style="@style/class_textView"
        />


    <TextView
        android:id="@+id/et_class_time_thu"
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="5dp"
        android:ems="10"
        android:layout_marginTop="29dp"
        android:layout_below="@+id/dt_class_time_thu"
        style="@style/Time_textView"
        />

    <TextView
        android:id="@+id/est_class_thu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:layout_marginStart="15dp"
        android:layout_alignBaseline="@+id/et_class_time_thu"
        android:layout_alignBottom="@+id/et_class_time_thu"
        android:layout_toEndOf="@+id/et_class_time_thu"
        style="@style/class_textView"
        />


</RelativeLayout>